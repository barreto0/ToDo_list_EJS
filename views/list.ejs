    <%- include("header") %> 
    <div class="box" id="heading">
    <h1><%= title %> </h1>
    </div>

    <div class="box">
        <% for(let i = 0; i<newListItems.length;i++){ %>
                <div class="item">
                    <input type="checkbox">
                    <p><%= newListItems[i] %> </p>
                </div>
        <% } %>

        <form class="item" action="/" method="POST">
            <input type="text" name="newItem" placeholder="Novo Item" autocomplete="off">
            <button type="submit" name="botaoLista" value= <%= title %> >+</button>
        </form>

    </div>
    <%- include("footer") %> 