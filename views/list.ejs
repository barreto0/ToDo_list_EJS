    <%- include("header") %> 
    <div class="box" id="heading">
    <h1><%= title %> </h1>
    </div>

    <div class="box">
        <form action="/delete" method="POST">
            <% for(let i = 0; i<newListItems.length;i++){ %>
                <div class="item">
                    <input type="checkbox" onchange="this.form.submit()" name="checkbox" value="<%=newListItems[i]._id%>">
                    <p><%= newListItems[i].name %> </p>
                </div>
            <% } %>
            <input type="hidden" name="listName" value="<%= title %>"></input>
        </form>

        <form class="item" action="/" method="POST">
            <input type="text" name="newItem" placeholder="Novo Item" autocomplete="off">
            <button type="submit" name="botaoLista" value= <%= title %> >+</button>
        </form>

    </div>
    <%- include("footer") %> 